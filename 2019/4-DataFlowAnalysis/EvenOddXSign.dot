graph G {
    nodesep=1;
    margin=0;
    rankdir="BT"

   subgraph cluster0{
      color=white;
    t;
    b -- eb;
    eb -- ep ;
    eb -- em ;
    em -- et;
    ep -- tp ;
    ep -- et ;
    et -- t;
    tp -- t;
    tdots -- t;
    b -- bdots;
    b;
    b [label="(⏊,⏊)"];
    t [label="(⏉,⏉)"];
    eb [label="(even,⏊)"]
       ep [label="(even,+)"];
       em [label="(even,-)"];
       tp [label="(⏉,+)"];
       et [label="(even,⏉)"];
       tdots [label="...",rank=3];
       bdots [label="..."];
   {rank = same; tdots; tp;}    
   {rank = same; bdots; eb;}    
   }

ep -- tdots [style=dotted];
bdots -- tdots [style=dotted];
       
   
}